   "source": [
    "def rev(string):\n",
    "    stack=[]\n",
    "    a=''\n",
    "    for i in string:\n",
    "        stack.append(i)\n",
    "    for j in range(len(stack)):\n",
    "        b=stack.pop()\n",
    "        a+=b\n",
    "    return a\n",
    "print(rev(\"palindrome\"))"
   ]
